void loop() { // Codigo principal a ser repetido indefinidamente.
 
 if (digitalRead(13)==HIGH){
 drawbitmap();
 }
 
 if(digitalRead(12)==HIGH){
   drawbitmap2();
  
  }
 
 if(digitalRead(12)==LOW && digitalRead(13)==LOW)
 {
  
  drawbitmap3();
  }
 
}
